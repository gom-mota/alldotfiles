/* Confirm Dialog */

@import "colors.rasi"

* {
    background-color:       @al;
    text-color:             @fg;
    font:							"Segoe UI Variable Static Text 11";
}

window {
    background-color:       @bg;
    width:      			225px;
    padding:    			25px;
    location:               center;
    y-offset:               -20px;
    
}

entry {
     background-color:       @al;
    expand: 				true;
    text-color:				@ac;
}
